    "@types/sinon": "^9.0.0",
    "chai": "^4.1.2",
    "css-loader": "^3.0.0",
    "file-loader": "^6.0.0",
    "imports-loader": "^0.8.0",
    "json-loader": "^0.5.7",
    "karma": "^5.0.2",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-mocha": "^2.0.0",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
