{
  "name": "bqplot",
  "version": "0.5.17",
  "description": "bqplot",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab notebook",
    "jupyterlab-extension"
  ],
  "author": {
    "name": "BQPlot Development team",
    "email": "bqplot@bloomberg.net"
  },
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/bloomberg/bqplot.git"
  },
  "scripts": {
    "build": "npm run build:css && npm run build:js && webpack --mode=production",
    "build:css": "lessc less/bqplot.less css/bqplot.css",
    "build:js": "tsc",
    "watch": "npm-run-all -p watch:*",
    "watch:js": "tsc --watch",
    "watch:nbextension": "webpack --watch --mode=development",
    "prepare": "npm run build",
    "test": "karma start --single-run"
  },
  "devDependencies": {
    "@jupyter-widgets/controls": "^1.5.0 || ^2",
    "@types/chai": "^4.1.7",
    "@types/d3": "^5.7.2",
