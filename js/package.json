    "typescript": "^3.5.2",
    "webpack": "^4.35.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^2 || ^3",
    "@phosphor/messaging": "^1",
    "@phosphor/widgets": "^1",
    "d3": "^5.7.0",
    "d3-selection-multi": "^1.0.1",
